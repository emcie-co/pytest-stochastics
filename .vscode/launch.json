{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: pytest [-]",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": ["-v", "--log-cli-level=DEBUG",],
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal"
        },
        {
            "name": "Python: pytest [disabled]",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
